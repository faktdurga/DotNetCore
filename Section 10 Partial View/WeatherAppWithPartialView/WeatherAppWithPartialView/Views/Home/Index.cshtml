﻿
@model IEnumerable<City>


<div class="container">
    <div class="page-content">
        <div class="margin-bottom">
            <h1>Weather</h1>
        </div>

        <div class="margin-top-200 margin-left">
            <div class="flex">
                @foreach (City city in Model)
                {
                    <partial name="_CityPartial" model="city"></partial>
                }
            </div>
        </div>
    </div>
</div>
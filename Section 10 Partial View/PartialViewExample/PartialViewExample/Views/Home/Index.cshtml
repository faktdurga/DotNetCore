

<h1>Home</h1>

<button class="button button-blue-back" type="button" id="btn_load">Load Programing Language</button>


<div class="box programming-languages-content">

</div>



<script>
    document.querySelector("#btn_load").addEventListener("click" , async function() {
    
      var response = await fetch("programming-language");

      var languages = await response.text();

        document.querySelector(".programming-languages-content").innerHTML = languages;

    });


</script>

